{
  "kind": "collectionType",
  "collectionName": "user_story_reports",
  "info": {
    "name": "User Story Reports"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "Reason": {
      "type": "richtext"
    },
    "user_story": {
      "model": "user-story",
      "via": "user_story_reports"
    },
    "username": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "reports"
    }
  }
}
